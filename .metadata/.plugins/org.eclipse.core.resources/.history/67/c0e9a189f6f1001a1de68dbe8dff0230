package com.stepdefinition;

import cucumber.api.java.After;
import cucumber.api.java.Before;

public class HookClass extends BaseAdactin {

	@Before(order=3,value="@Reg")
	public void beforeHook1() {
		System.out.println("beforeHook1");
	}
	@Before(order=4)
	public void beforeHook2() {
		System.out.println("beforeHook2");
	}
	@Before(order=1)
	public void beforeHook3() {
		System.out.println("beforeHook3");
	}
	@Before(order=2)
	public void beforeHook4() {
		System.out.println("beforeHook4");
	}
	@After(order=3)
	public void afterHook1() {
		System.out.println("afterHook1");
	}
	@After(order=2)
	public void afterHook2() {
		System.out.println("afterHook2");
	}
	@After(order=4)
	public void afterHook3() {
		System.out.println("afterHook3");
	}
	@After(order=1)
	public void afterHook4() {
		System.out.println("afterHook4");
	}

	
	
	
	
	
	
	
	
	
	
	
